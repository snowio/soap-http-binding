{
    "name": "snowio/soap-http-binding",
    "description": "A PHP library for binding SOAP messages to PSR-7 HTTP messages.",
    "keywords": ["SOAP", "HTTP", "PSR-7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Meng Tian",
            "email": "tianmeng94@hotmail.com"
        }
    ],
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/snowio/"},
        {"packagist.org": false}
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-diactoros": "~1.3",
        "snowio/php-soap-interpreter": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "SnowIO\\Soap\\HttpBinding\\": "src/"
        }
    }
}
